{
    "name": "Multimedia detector",
    "version": "1.5",
    "author": "Borllor Li",
    "description": "It is used to detecting media multimedia resource.",
    "permissions": [
        "declarativeContent",
        "storage",
        "background",
        "tabs",
        "history",
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
    ],
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Youtube Detector",
        "default_icon": {
            "16": "images/16.png",
            "32": "images/32.png",
            "48": "images/48.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "manifest_version": 2,
    "homepage_url": "https://github.com/borllor"
}